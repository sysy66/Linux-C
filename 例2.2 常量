#include <stdio.h>
/*
* 2.2 常量（Constant）
*/
int main(void)
{
	printf("character: %c\ninteger: %d\nfloating point: %f\n", '}', 34, 3.14);

	printf("character %% : %c\n", '%');

	printf("character: %c\n", '\"');
	printf("character: %c\n", '"');
	printf("character: %c\n", '\?');
	printf("character: %c\n", '?');

	printf("character: %c\n", '\\');
	printf("character: %c\n", '\'');

	return 0;
}